        },
        "framework-kendryte210-standalone-sdk": {
            "type": "framework",
            "optional": true,
            "version": "~0.5.4"
        },
        "tool-kflash-kendryte210": {
            "type": "uploader",
            "optional": true,
            "version": "~0.6.0"
        },
        "tool-openocd-kendryte210": {
